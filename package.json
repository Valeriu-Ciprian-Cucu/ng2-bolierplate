{
  "name": "ng2-browserify",
  "version": "1.0.0",
  "description": "Angular 2 boilerplate using Browserify",
  "scripts": {
    "build": "browserify -t [ stringify --extensions [ .html ] ] -p [ tsify ] src/main.ts -o dist/main.js"
  },
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "browserify": "^13.1.1",
    "stringify": "^5.1.0",
    "tsify": "^2.0.3",
    "typescript": "^2.0.10"
  },
  "author": "Valeriu Cucu",
  "license": "ISC"
}
